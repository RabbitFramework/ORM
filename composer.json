{
    "name": "rabbitphp/orm",
    "description": "Object relational mapping component of RabbitPHP framework",
    "type": "component",
    "require": {
        "rabbitphp/dependency-injector": "*",
        "rabbitphp/file": "*",
        "rabbitphp/mvc": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kaktus",
            "email": "kaktus@xirion.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rabbit\\ORM\\": "src",
            "Rabbit\\ORM\\Drivers\\": "src/Drivers",
            "Rabbit\\ORM\\Builders\\": "src/Builders",
            "Rabbit\\ORM\\Entity\\": "src/Entity",
            "Rabbit\\ORM\\Repository\\": "src/Repository"
        },
        "files": [
            "src/globals.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
